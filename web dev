<script> 
function Validation(){ 
var num1=document.getElementById('first').value; 
var num2=document.getElementById('second').value; 
var operator=document.getElementById('op').value; 
if(operator=='+')   { 
var res=Number(num1)+Number(num2); 
document.getElementById('loc').innerHTML=res; 
} 
if(operator=='-'){ 
var res=Number(num1)-Number(num2); 
document.getElementById('loc').innerHTML=res;    
} 
if(operator=='*'){ 
var res=Number(num1)*Number(num2); 
document.getElementById('loc').innerHTML=res;   
} 
if(operator=='/')  { 
var res=Number(num1)/Number(num2); 
document.getElementById('loc').innerHTML=res;   
} 
} 
</script> 
</body> 
</html>

